{
  "name": "template-hardhat",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "npx hardhat test",
    "compile": "npx hardhat compile",
    "coverage": "npx hardhat coverage",
    "typechain": "npx hardhat typechain",
    "deploy:local": "npx hardhat run --network local scripts/HelloWorld.ts",
    "test:StakingSoil": "npx hardhat test ./test/StakingSoil.ts",
    "GenerateWallet": "npx hardhat run scripts/GenerateWallet.ts",
    "deployLocal:Soil": "npx hardhat run --network local scripts/Soil.ts",
    "deployLocal:StakingSoil": "npx hardhat run --network local scripts/StakingSoil.ts",
    "configurationLocal": "npx hardhat run --network local scripts/Config.ts",
    "deployLocal": "yarn deployLocal:Soil && yarn deployLocal:StakingSoil && yarn configurationLocal",
    "deploy:Soil": "npx hardhat run --network bsc scripts/Soil.ts",
    "deploy:StakingSoil": "npx hardhat run --network bscTestnet scripts/StakingSoil.ts",
    "configuration": "npx hardhat run --network bscTestnet scripts/Config.ts",
    "deploy": "yarn deploy:Soil && yarn deploy:StakingSoil && yarn configuration"
  },
